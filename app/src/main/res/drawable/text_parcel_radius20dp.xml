<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <!-- 填充颜色 -->
    <solid android:color="@color/white" />

    <!-- 线的宽度，颜色灰色 -->
    <stroke
        android:width="@dimen/dimen_1dp"
        android:color="@color/yellow_f7"
        android:dashGap="@dimen/dimen_2dp"
        android:dashWidth="@dimen/dimen_2dp" />

    <!-- 矩形的圆角半径 -->
    <corners android:radius="@dimen/dimen_20dp" />

    <padding
        android:bottom="@dimen/dimen_4dp"
        android:left="@dimen/dimen_8dp"
        android:right="@dimen/dimen_8dp"
        android:top="@dimen/dimen_4dp" />
</shape>